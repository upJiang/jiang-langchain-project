{
  "name": "langchain-weather-demo",
  "version": "1.0.0",
  "description": "LangChain.js demo with weather forecast functionality",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "start:all": "npm run build && node server.js",
    "dev:server": "nodemon --watch src --ext ts,js --exec ts-node src/index.ts",
    "dev:web": "nodemon --watch server.js --watch frontend --ext js,html,css --exec node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "langchain",
    "rag",
    "typescript",
    "weather",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.3",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@langchain/community": "^0.0.36",
    "@langchain/core": "^0.1.25",
    "@langchain/openai": "^0.0.14",
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.7.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "langchain": "^0.1.17",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  }
}
